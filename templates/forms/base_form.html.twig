<div class="card card-body p-3">
    {{ form_start(form) }}
    {% block start %}{% endblock %}

    <h3 class="card-title mb-3 d-flex justify-content-between">
        {% block title %}{% endblock %}
        <i class="ajax-form-check"></i>
    </h3>

    {% block end %}{% endblock %}
    {{ form_rest(form) }}
    {{ form_end(form) }}
</div>